@model CapaEntidad.EntUsuario





@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Quicksand:wght@300;500;700&display=swap" rel="stylesheet">
    <link href="~/Content/css/Usuario/MensajeRecuperacion.css" rel="stylesheet" />


    <title>Mensaje de Recuperación</title>
</head>
<body>
    <div class="login">
        <div class="form-container">
      
            <img src="~/Models/Logos/logo-1.png" alt="logo" class="logo">

            <h1 class="title">E-mail ha sido enviado!</h1>
            <p class="subtitle">Consulte su bandeja de entrada para obtener instrucciones sobre cómo restablecer la contraseña.</p>

            <div class="email-image">
                <img src="~/Models/icons/email.svg" alt="email">
            </div>

            <button class="primary-button login-button"> Acceso </button>

            <p class="resend">
                <span>¿No recibiste el correo electrónico?</span>
                <a href="/">reenviar</a>
            </p>
        </div>
    </div>
</body>
</html>
